{
    "name": "laravelio/portal",
    "description": "The source code for the Laravel.io community portal.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "bugsnag/bugsnag-laravel": "^2.14",
        "doctrine/dbal": "^2.5",
        "facade/ignition": "^1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^1.1",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "lasserafn/php-initial-avatar-generator": "^2.0",
        "league/commonmark": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-backup": "^5.10",
        "spatie/laravel-robots-middleware": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.2",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.6",
        "laravel/browser-kit-testing": "^4.0",
        "laravel/homestead": "^7",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpspec/phpspec": "^5.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "resources/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "setup": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate --seed",
            "npm install",
            "npm run dev"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
